{
    "name": "robocoder/app",
    "description": "base application for new roles",
    "keywords": [
    ],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Anthon Pang",
            "email": "apang@softwaredevelopment.ca"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "nikic/fast-route": "^1.3",
        "geoip2/geoip2": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 3600,
        "secure-http": false,
        "sort-packages": true,
        "discard-changes": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
